{
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },

  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": ["react", "react-hooks", "import"],
  "rules": {
    "import/no-unresolved": "error"
  }
}
